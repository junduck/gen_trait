{
  "include": ["<tuple>"],
  "namespace": "jduck::gen_trait::sample",
  "trait": [
    {
      "name": "pack_and_get",
      "template": [
        {
          "type": "size_t",
          "name": "I"
        },
        {
          "type": "typename",
          "name": "Args",
          "pack": true
        }
      ],
      "func": [
        {
          "name": "pack",
          "ret": "std::tuple<Args...>",
          "args": [
            {
              "name": "args",
              "type": "Args",
              "wrap": "std::move"
            }
          ]
        },
        {
          "name": "get",
          "ret": "int",
          "args": [],
          "cvref": "const noexcept"
        }
      ]
    },
    {
      "name": "sendable",
      "func": [
        {
          "name": "send",
          "ret": "void",
          "args": [
            {
              "name": "x",
              "type": "int",
              "cvref": "const&"
            }
          ]
        }
      ]
    }
  ]
}
