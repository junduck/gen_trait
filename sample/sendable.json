{
  "include": ["<string>", "<vector>"],
  "namespace": "jduck::gen_trait::sample",
  "trait": [
    {
      "name": "sendable",
      "func": [
        {
          "name": "send",
          "ret": "bool",
          "args": [{ "name": "msg", "type": "std::string", "cvref": "const &" }]
        },
        {
          "name": "send",
          "ret": "bool",
          "args": [{ "name": "msg", "type": "std::vector<char>", "wrap": "std::move" }]
